    "test": "jest --color --config jest.config.json",
    "serve": "npx styleguidist server"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
