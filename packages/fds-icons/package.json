{
  "name": "fds-icons",
  "version": "1.19.0",
  "description": "Custom SVG icons for Form Design System",
  "author": "CB Insights",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "setupDist": "rm -rf ./dist && mkdir ./dist",
    "export": "node ./scripts/exportFromSketch.js",
    "dist:optimize": "node ./scripts/optimize.js",
    "dist:react": "node ./scripts/createComponents.js",
    "dist:compile": "babel ./dist/react/ --out-dir ./dist/react/",
    "build:dist": "yarn setupDist && yarn dist:optimize && yarn dist:react && yarn dist:compile",
    "build:docs": "node ./scripts/buildDocs.js",
    "build": "yarn build:dist && yarn build:docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cbinsights/form-design-system.git"
 },
  "dependencies": {
    "classnames": "^2",
    "fds-dictionary": "^1.19.22",
    "prop-types": "^15"
  }
}
